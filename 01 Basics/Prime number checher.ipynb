   "source": [
    "def is_prime(n):\n",
    "    if n < 2:\n",
    "        print(n, 'is not prime')\n",
    "    for i in range(2, n):\n",
    "        if n % i:\n",
    "            print(n, 'is prime')\n",
    "            return\n",
    "    print(n, 'is not prime')\n",
    "    return"
   ]
